				http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context-2.5.xsd">


    <context:annotation-config/>
    <context:component-scan base-package="com.spring_1_100.test_41_50.test48"></context:component-scan>


    <bean id="teacher" class="com.spring_1_100.test_41_50.test48.Teacher">
        <qualifier  value="teacherxxxxxxx"></qualifier>
    </bean>

    <bean id="student" class="com.spring_1_100.test_41_50.test48.Student">

        <qualifier   value="studentxxxxxx">
        </qualifier>
    </bean>


</beans>


